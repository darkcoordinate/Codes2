using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace WpfApplication1
{
  [Serializable]
    class datasheet
    {
        protected double sabji=0;
        protected double fruits=0;
        protected double ediblekitchen=0;
        protected double nonedibles=0;
        protected double households=0;
        protected double stationary=0;
        protected double petrol=0;
        protected double mobile=0;
        protected double maintanence=0;
        protected double cmo=0;
        protected double generator=0;
        protected double divyaexpense=0;
        protected double medication=0;
        protected double milk;
        protected double news;
        protected double dth=0;
        public DateTime Currentdate;
        protected string cmoatti;
        protected string milkatti;
        protected string dthatti;
        protected string generatoratti;
        protected string maintinanceatti;
        protected string petrolatti;

        public datasheet()
        {
            Currentdate = DateTime.Now;
            news = 9F;
        }

        ~datasheet()
        {
        }

        public double adtodaysabji
        {
            get
            {
                return sabji;
            }
            set
            {
                sabji = value;
            }
        }

        public double adtodayfruits
        {
            get
            {
                return fruits;
            }
            set
            {
                fruits = value;
            }
        }

        public double adtodayedible
        {
            get
            {
                return ediblekitchen;
            }
            set
            {
                ediblekitchen = value;
            }
        }

        public  double adtodaynonedible
        {
            get
            {
                return nonedibles;
            }
            set
            {
                nonedibles = value;
            }
        }

        public double adtodayhouseholds
        {
            get
            {
                return households;
            }
            set
            {
                households = value;
            }
        }

        public double adtodaystationary
        {
            get
            {
                return stationary;
            }
            set
            {
                stationary = value;
            }
        }

        public double adtodaypetrol
        {
            get
            {
                return petrol;
            }
            set
            {
                petrol = value;
            }
        }

        public double adtodaymobile
        {
            get
            {
                return mobile;
            }
            set
            {
                mobile = value;
            }
        }

        public double adtodaymaintinance
        {
            get
            {
                return maintanence;
            }
            set
            {
                maintanence = value;
            }
        }

        public double adtodaycmo
        {
            get
            {
                return cmo;
            }
            set
            {
                cmo = value;
            }
        }

        public double adtodaygenerator
        {
            get
            {
                return generator;
            }
            set
            {
                generator = value;
            }

        }
        public double adtodaydivya
        {
            get
            {
                return divyaexpense;
            }
            set
            {
                divyaexpense = value;
            }
        }

        public double adtodaymedication
        {
            get
            {
                return medication;
            }
            set
            {
                medication = value;
            }
        }

        public double adtodaymilk
        {
            get
            {
                return milk;
            }
            set
            {
                milk = value;
            }
        }

        public double adtodaynews
        {
            get
            {
                return news;
            }
            set
            {
                news = value;
            }
        }

        public double adtodaydth
        {
            get
            {
                return dth;
            }
            set
            {
                dth = value;
            }
        }

        public string adcmoati
        {
            get
            {
                return cmoatti;
            }
            set
            {
                cmoatti = value;
            }
        }

        public string admilkati
        {
            get
            {
                return milkatti;
            }
            set
            {
                milkatti = value;
            }
        }

        public string addthati
        {
            get
            {
                return  dthatti;
            }
            set
            {
                dthatti = value;
            }
        }

        public string adgen
        {
            get
            {
                return generatoratti;
            }
            set
            {
                generatoratti = value;
            }
        }

        public string admaintainati
        {
            get
            {
                return maintinanceatti;
            }
            set
            {
                maintinanceatti = value;
            }
        }

        public string adpetrolati
        {
            get
            {
                return petrolatti;
            }
            set
            {
                petrolatti = value;
            }
        }

        public double getotalday()
        {
            return sabji + fruits + ediblekitchen + nonedibles + households + stationary + petrol + mobile + maintanence + cmo + generator + divyaexpense + medication + milk + news + dth ;
        }
    }        
}
